from qsort import Qsort, qsort
import bag

const NITEMS = 4
const values = {1..NITEMS}

a = [ choose(values) for i in {1..choose({1..NITEMS})} ]
testqs = Qsort(a)
sa = qsort(?testqs)
assert all(sa[i - 1] <= sa[i] for i in {1..len(sa)-1}) # sorted?
assert bag.fromList(a) == bag.fromList(sa); # is it a permutation?
