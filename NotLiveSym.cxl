# symmetric but non-live solution to mutual exclusion problem

def p(self):
    while True:
        @ncs: assert True, True;
        flags[self] = True;
        while flags[1 - self]:
            pass;
        ;
        @cs: assert (atLabel "cs") == dict{ nametag(): 1 }, atLabel("cs");
        flags[self] = False;
    ;
;
flags = dict{ 0: False, 1: False };
spawn p(0), 0;
spawn p(1), 1;
