def Qenqueue(q, v):
    let node = malloc(dict{ .value: v, .next: None }):
        lock(?q->lock);
        if q->head == None:
            q->head = q->tail = node;
        else:
            q->tail->next = node;
            q->tail = node;
        ;
        unlock(?q->lock);
    ;
;
def Qdequeue(q):
    lock(?q->lock);
    let node = q->head:
        if node == None:
            result = ();
        else:
            result = (node->value,);
            q->head = node->next;
            free(node);
        ;
    ;
    unlock(?q->lock);
;
