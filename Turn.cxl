def p(self):
    while True:
        @ncs: assert canReachLabel(dict{ .name: "p", .tag: 1 - self }, "cs")

        # Enter critical section
        while turn == (1 - self):
            pass;
        ;

        # Critical section
        @cs: assert (atLabel "cs") == dict{ nametag(): 1 }, atLabel("cs");

        # Leave critical section
        turn = 1 - self;
    ;
;
turn = 0;
spawn p(0), 0;
spawn p(1), 1;
