const MustReach =
    {
        dict{
            .process: dict{ .name: "p", .tag: tag },
            .label: "cs"
        } for tag in 0..1
    }
;

def p(self):
    while True:
        # Enter critical section
        while turn == (1 - self):
            pass;
        ;

        # Critical section
        @cs: assert (atLabel "cs") == dict{ nametag(): 1 }, atLabel("cs");

        # Leave critical section
        turn = 1 - self;
    ;
;
turn = 0;
spawn p(0), 0;
spawn p(1), 1;
