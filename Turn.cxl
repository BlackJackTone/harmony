def p(self):
    while choose({ False, True }):
        @ncs: pass;

        # Enter critical section
        while turn == (1 - self):
            pass;
        ;

        # Critical section
        @cs: assert (atLabel "cs") == dict{ nametag(): 1 }, atLabel("cs");

        # Leave critical section
        turn = 1 - self;
    ;
;
turn = 0;
spawn p(0), 0;
spawn p(1), 1;
