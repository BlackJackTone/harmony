import spinlock;

def checkInvariant():
    let sum = 0:
        if not shared:
            sum = 1;
        ;
        for i in {0..N-1} such that not private[i]:
            sum += 1;
        ;
        result = sum <= 1;
    ;
;
def invariantChecker():
    assert checkInvariant();
;

spawn invariantChecker();
