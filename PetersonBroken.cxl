# Broken Peterson's algorithm by swapping first two assignments

def p(self):
    while choose({ False, True }):
        turn = 1 - self;
        flags[self] = True;
        while flags[1 - self] and (turn == (1 - self)):
            pass;
        ;
        @cs: assert (atLabel "cs") == dict{ nametag(): 1 }, atLabel("cs");
        flags[self] = False;
    ;
;
flags = dict{ 0: False, 1: False };
turn = 0;
atomic:
    spawn p(0), 0;
    spawn p(1), 1;
;
