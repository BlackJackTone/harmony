# Broken Peterson's algorithm by swapping first two assignments

def process(self):
    while choose({ False, True }):
        turn = 1 - self;
        flags[self] = True;
        await (not flags[1 - self]) or (turn == self);
        @cs: assert atLabel.cs == dict{ nametag(): 1 }, atLabel.cs;
        flags[self] = False;
    ;
;
flags = [ False, False ];
turn = choose({0, 1});
spawn process(0);
spawn process(1);
