# Broken Peterson's algorithm by swapping first two assignments

def process(self):
    while choose({ False, True }):
        turn = 1 - self;
        flags[self] = True;
        while flags[1 - self] and (turn == (1 - self)):
            pass;
        ;
        @cs: assert atLabel.cs == dict{ nametag(): 1 };
        flags[self] = False;
    ;
;
flags = [ False, False ];
turn = 0;
atomic:
    spawn process(0), 0;
    spawn process(1), 1;
;
