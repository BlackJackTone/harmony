lock = False

def tas(s):
    atomic:
        result = !s
        !s = True

def clear(s):
    atomic:
        assert !s
        !s = False
    
def thread():
    while choose({ False, True }):
        while tas(?lock):
            pass
        @cs: assert atLabel(cs) == { (thread, ()): 1 }
        clear(?lock)
    
for i in {1..10}:
    spawn thread()
