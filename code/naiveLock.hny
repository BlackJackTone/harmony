lockTaken = False;

def process(self):
    while choose({ False, True }):
        # Enter critical section
        await not lockTaken;
        lockTaken = True;

        # Critical section
        @cs: assert atLabel.cs == dict{ nametag(): 1 };

        # Leave critical section
        lockTaken = False;
    ;
;

spawn process(0);
spawn process(1);
