import synch;

const N = 5;

def diner(which):
    while choose({ False, True }):
        let left = which, right = (which % N) + 1:
            call lock &(forks[left]);
            call lock &(forks[right]);
            # dine
            call unlock &(forks[left]);
            call unlock &(forks[right]);
            # think
        ;
    ;
;
forks = dict{ Lock() for i in 1..N };
for i in 1..N:
    spawn diner(i);
;
