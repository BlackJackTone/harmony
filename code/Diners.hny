from synch import Lock, acquire, release;

const N = 5;

forks = dict{ Lock() for i in {1..N} };

def diner(which):
    let left, right = (which, (which % N) + 1):
        while choose({ False, True }):
            acquire(?forks[left]);
            acquire(?forks[right]);
            # dine
            release(?forks[left]);
            release(?forks[right]);
            # think
        ;
    ;
;
for i in {1..N}:
    spawn diner(i);
;
