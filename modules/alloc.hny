import synch

pool = []
flist = None;           # free list
lock = synch.Lock()

def malloc(v):
    synch.acquire(?lock)
    if flist == None:
        let i = len(pool):
            pool[i] = None
            result = ?pool[i]
    else:
        result = flist
        flist = !result
    !result = v
    synch.release(?lock)

def free(r):
    synch.acquire(?lock)
    !r = flist
    flist = r
    synch.release(?lock)
