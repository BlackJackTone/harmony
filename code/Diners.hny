import synch;

const N = 5;

def diner(which):
    let left, right = (which, (which % N) + 1):
        while choose({ False, True }):
            lock(?forks[left]);
            lock(?forks[right]);
            # dine
            unlock(?forks[left]);
            unlock(?forks[right]);
            # think
        ;
    ;
;
forks = dict{ Lock() for i in {1..N} };
for i in {1..N}:
    spawn diner(i);
;
