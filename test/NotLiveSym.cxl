# symmetric but non-live solution to mutual exclusion problem

def process(self):
    while choose({ False, True }):
        flags[self] = True;
        while flags[1 - self]:
            pass;
        ;
        @cs: assert atLabel.cs == dict{ nametag(): 1 };
        flags[self] = False;
    ;
;
flags = dict{ 0: False, 1: False };
spawn process(0), 0;
spawn process(1), 1;
