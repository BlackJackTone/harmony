def process(self):
    while choose({ False, True }):
        # Enter critical section
        while turn == (1 - self):
            pass;
        ;

        # Critical section
        @cs: assert (atLabel "cs") == dict{ nametag(): 1 };

        # Leave critical section
        turn = 1 - self;
    ;
;
turn = 0;
spawn process(0), 0;
spawn process(1), 1;
