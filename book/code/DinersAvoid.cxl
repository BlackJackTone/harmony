import synch;

const N = 5;

def diner(which):
    let left = which, right = (which % N) + 1:
        while choose({ False, True }):
            P(&sema);
            lock(&forks[left]);
            lock(&forks[right]);
            # dine
            unlock(&forks[left]);
            unlock(&forks[right]);
            V(&sema);
            # think
        ;
    ;
;
forks = dict{ Lock() for i in 1..N };
sema = Semaphore(N - 1);
for i in 1..N:
    spawn diner(i);
;
